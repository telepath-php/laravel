{
    "name": "telepath/laravel",
    "description": "Laravel integration of Telepath",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0 | ^10.0",
        "telepath/telepath": "~0.10",
        "symfony/cache": "^6.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Telepath\\Laravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tii",
            "email": "mail@tii.one"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "0.3.x-dev"
        },
        "laravel": {
            "providers": [
                "Telepath\\Laravel\\TelepathServiceProvider"
            ],
            "aliases": {
                "Telepath": "Telepath\\Laravel\\Facades\\Telepath"
            }
        }
    },
    "require-dev": {
        "laravel/framework": "^9.0 | ^10.0"
    }
}
